{
  "mcpServers": {
    "memory": {
      "command": "python",
      "args": [
        "-m",
        "mcp.server.memory"
      ],
	"env": {
                "MEMORY_FILE_PATH": "${workspaceFolder}/.vscode/memory.json"
            },
      "disabled": false
    },
    "sequentialthinking": {
      "command": "python", 
      "args": [
        "-m",
        "mcp.server.sequentialthinking"
      ],
      "env": {},
      "disabled": false
    },
    "filesystem": {
      "command": "python",
      "args": [
        "-m", 
        "mcp.server.filesystem",
        "${workspaceFolder}"
      ],
      "env": {},
      "disabled": false
    },
    "sqlite": {
      "command": "python",
      "args": [
        "-m",
        "mcp.server.sqlite",
        "${workspaceFolder}/db.sqlite3"
      ],
      "env": {},
      "disabled": false
    },
    "git": {
      "command": "python",
      "args": [
        "-m",
        "mcp.server.git",
        "${workspaceFolder}"
      ],
      "env": {},
      "disabled": false
    },
    "python": {
      "command": "python",
      "args": [
        "-m",
        "mcp.server.python"
      ],
      "env": {},
      "disabled": false
    }
  }
}